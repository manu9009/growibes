    <div class='field__items row'>
  {% endif %}

  {% for item in items %}
    <div class="more-link-outline"><a class="btn btn-outline-primary btn-download" href="{{ item.content|render|striptags|trim }}" download>{% trans %} Download {% endtrans %} </a></div>
  {% endfor %}

  {% if multiple %}
    </div>
  {% endif %}
</div>
